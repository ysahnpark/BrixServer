{
    "numWorkers": 2,
    "maxDeadWorkerSize": 10,
    "host": "localhost",
    "port": 8088,
    "maxSockets": 100,

    "redisHost": "127.0.0.1",
    "redisPort": "6379",
    // Whether or not to allow flushing  of the Redis cache
    "cacheAllowFlush": true,

    // The AMS base URL, pointing to local test machine.
    "amsBaseUrl": "http://localhost:9080",
    // The AMS's NodeResult submission endpoint path
    "amsSubmissionPath": "/las-paf/sd/paf-service/ip/brix/nodresult",
    "amsCaching": true,

    "hubBaseUrl": "http://localtest",
    "bipsBaseUrl": "http://localhost:8088",
    "ceBaseUrl": "http://localhost:8090",

    // Log related settings
    "logLevel": "debug",
    "logToScreen": true,
    "logToFile": true,
    "logDir": "../log",
    "logAllowWebAccess": true,

    // Image related settings
    "imgDir": "./images",
    "imgBaseUrl": "http://localhost:8088/images/"
}
